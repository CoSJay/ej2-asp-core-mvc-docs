@using Syncfusion.EJ2
@using Syncfusion.EJ2.Schedule

@{
    var dataManager = new DataManager() { 
        Url = "https://localhost:44360/api",
        Adaptor = "ODataV4Adaptor",
        CrossDomain = true 
    };
}

<ejs-schedule id="schedule" width="100%" height="550" selectedDate="new DateTime(2022, 12, 12)" readonly="true">
    <e-schedule-eventsettings dataSource="dataManager" query="new ej.data.Query().from('Events').addParams('readOnly', 'true')" includeFiltersInQuery="true">
    </e-schedule-eventsettings>
</ejs-schedule>